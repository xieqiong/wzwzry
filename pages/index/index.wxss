/**index.wxss**/
.image-bak{
  width: 100%;
  height: 422rpx;
}
.team-box{
  width: 100%;
  min-height: calc(100vh - 422rpx);
  padding: 40rpx;
  box-sizing:border-box;
}
.time-title{
  height: 70rpx;
  width: 100%;
  text-align: center;
  line-height: 70rpx;
  color: #fff;
  margin-top: 10rpx;
  font-size: 24rpx;
  border-bottom: 1rpx solid #f1f2f3;
}
.team-item{
  width: 100%;
  height: 200rpx;
}
.member-avatar{
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
  border: 1rpx solid #ddd;
  margin-top: 30rpx;
  vertical-align: top;
  border-radius: 50rpx;
}
.member-name{
  display: inline-block;
  width: 450rpx;
  height: 150rpx;
  margin-top: 30rpx;
  color: #ddd;
  vertical-align: top;
  margin-left: 30rpx;
}
.member-name-title{
  line-height: 50rpx;
  font-size: 35rpx;
  padding: 10rpx 0;
}
.member-name-position{
  font-size: 30rpx;
  margin-top: 35rpx;
}
.member-name-levle{
  margin-left:20rpx;
  color: red;
  font-size: 30rpx;
}
